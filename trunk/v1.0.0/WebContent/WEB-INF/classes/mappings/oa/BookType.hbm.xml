<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.youthor.bsp.oa.model.BookType" table="OA_BOOK_TYPE"  >
		<id name="id" type="string"  column="id">
          <generator class="uuid.hex"/>
        </id>
        <property name="name" type="string" >
            <column name="name"  />
        </property>
        <property name="parentId" type="string">
            <column name="parent_Id" />
        </property>
        
        <property name="allParentId" type="string">
            <column name="all_Parent_Id" />
        </property>
        
        <property name="allParentName" type="string" >
            <column name="all_Parent_Name" />
        </property>
        
         <property name="orderIndex" type="float" >
            <column name="order_index" />
        </property>
        
        <property name="deleteFlag" type="string" >
            <column name="delete_flag" />
        </property>
       
    </class>

</hibernate-mapping>

