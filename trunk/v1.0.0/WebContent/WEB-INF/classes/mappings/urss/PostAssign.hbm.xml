<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-cascade="save-update" package="com.youthor.bsp.core.urss.model">
	<class name="PostAssign" table="URSS_POST_ASSIGN">
		<id name="assignId" type="string" unsaved-value="">
			<column name="ASSIGN_ID" length="32"/>
			<generator class="uuid.hex"/>
		</id>
		 	      
	    	       <property name="orgId" column="ORG_ID" type="string"/>
	    	       <property name="postId" column="POST_ID" type="string"/>
	    	       <property name="orderIndex" column="ORDER_INDEX" type="float"/>
	    	
	</class>
</hibernate-mapping>