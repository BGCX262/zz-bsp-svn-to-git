<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-cascade="save-update" package="com.youthor.bsp.core.wfss.model">
    <class name="WorkItemDoing" table="WFSS_WORK_ITEM_DOING">
    
        <id name="workItemId" type="string" unsaved-value="">
            <column name="work_Item_Id" length="32"/>
            <generator class="uuid.hex"/>
        </id>
        <property name="processInstanceId" column="process_Instance_Id" type="string" />
        <property name="activityInstanceId" column="activity_Instance_Id" type="string" />
        <property name="parentActivityInstId" column="parent_Activity_Inst_Id" type="string" />
        <property name="activityId" column="activity_Id" type="string" />
        <property name="activityName" column="activity_Name" type="string" />
        <property name="appId" column="app_Id" type="string" />
        <property name="appFormId" column="app_Form_Id" type="string" />
        <property name="senderId" column="sender_Id" type="string" />
        <property name="senderName" column="sender_Name" type="string" />
        <property name="ownerId" column="owner_Id" type="string" />
        <property name="ownerName" column="owner_Name" type="string" />    
        <property name="createDate" column="create_Date" type="java.util.Date" />
        <property name="isRead" column="is_Read" type="string"/>
        <property name="processId" column="process_Id" type="string" />
        <property name="processName" column="process_Name" type="string" />
        <property name="title" column="title" type="string"/>
        <property name="states" column="states" type="string" />
        <property name="isAgentedOrgId" column="IS_AGENTED_ORG_ID" type="string"/>
        <property name="isAgentedOrgName" column="IS_AGENTED_ORG_NAME" type="string"/>
        
    </class>
</hibernate-mapping> 