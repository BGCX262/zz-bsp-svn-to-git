<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-cascade="save-update" package="com.youthor.gxunion.core.model">
    <class name="SimpleMaterials" table="GXUNION_SIMPLE_MATERIALS">
        <id name="id" type="string" unsaved-value="">
            <column name="id" length="32"/>
            <generator class="uuid.hex"/>
        </id>
                   <property name="code" column="CODE" type="string"/>
                   <property name="name" column="NAME" type="string"/>
                   <property name="inventory" column="INVENTORY" type="float"/>
                   <property name="warningMinValue" column="WARNING_MIN_VALUE" type="float"/>
                   <property name="principalEmail" column="PRINCIPAL_EMAIL" type="string"/>
                   <property name="status" column="STATUS" type="string"/>
    </class>
</hibernate-mapping>
